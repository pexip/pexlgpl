#include <windows.h>
#include <winver.h>

VS_VERSION_INFO VERSIONINFO
  FILEVERSION @MAJOR_VERSION@,@MINOR_VERSION@,@MICRO_VERSION@,@REVISION_VERSION@
  PRODUCTVERSION @MAJOR_VERSION@,@MINOR_VERSION@,@MICRO_VERSION@,@REVISION_VERSION@
  FILEFLAGSMASK 0
  FILEFLAGS 0
  FILEOS VOS__WINDOWS32
  FILETYPE VFT_DLL
  FILESUBTYPE VFT2_UNKNOWN
  BEGIN
    BLOCK "StringFileInfo"
    BEGIN
      BLOCK "040904B0"
      BEGIN
        VALUE "CompanyName", "@COMPANY_NAME@"
        VALUE "ProductName", "@PRODUCT_NAME@"
        VALUE "ProductVersion", "@MAJOR_VERSION@.@MINOR_VERSION@.@MICRO_VERSION@.@REVISION_VERSION@"
        VALUE "LegalCopyright", "@COPYRIGHT@"
        VALUE "FileDescription", "@FILE_DESCRIPTION@"
        VALUE "FileVersion", "@MAJOR_VERSION@.@MINOR_VERSION@.@MICRO_VERSION@.@REVISION_VERSION@"
        VALUE "InternalName", "@INTERNAL_NAME@"
      END
    END
    BLOCK "VarFileInfo"
    BEGIN
      VALUE "Translation", 0x409, 1200
    END
  END